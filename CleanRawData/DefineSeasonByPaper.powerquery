let
    out = (month as number) as text => 
    if (month = 1) or (month = 2) or (month = 12) then "DJF" 
    else if (month = 3) or (month = 4) or (month = 5) then "MAM"
    else if (month = 6) or (month = 7) or (month = 8) then "JJA"
    else if (month = 9) or (month = 10) or (month = 11) then "SON"
    else null 
in
    out
